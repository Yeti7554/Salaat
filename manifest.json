{
    "name": "Prayer Times",
    "version": "0.1",
    "description": "A simple app that displays daily prayer times for a selected your city",
    "manifest_version": 2,
    "icons": {
      "128": "128.png"
    },
    "browser_action": {
      "default_icon": {
        "128": "128.png"
      },
      "default_popup": "index.html"
    },
    "permissions": [
      "https://api.aladhan.com/v1/timingsByCity",
      "activeTab"
    ]
  }
  